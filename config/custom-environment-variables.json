{
  "app": {
    "port": "SERVICE_PORT",
    "mountUrl": "MOUNT_URL"
  },
  "services": {
    "cookieHandler": {
      "domain": "COOKIE_DOMAIN",
      "path": "COOKIE_PATH"
    },
    "claimService": {
      "url": "CLAIM_URL"
    },
    "securityNumberService": {
      "url": "TOTP_URL",
      "skipValidation": "TOTP_SKIP_VALIDATION"
    },
    "addressLookup": {
      "url": "LOCATION_URL",
      "contextPath": "LOCATION_CONTEXT_PATH",
      "ca": "INTEGRATION_GATEWAY_CA",
      "proxy": "INTEGRATION_GATEWAY_PROXY"
    }
  },
  "langaugeToggle": {
    "showWelshLanguageToggle": {
      "__name": "SHOW_WELSH_LANGUAGE_TOGGLE",
      "__format": "boolean"
    }
  },
  "session": {
    "secure": {
      "cookie": {
        "__name": "SESSION_SECURE_COOKIE",
        "__format": "boolean"
      }
    },
    "length": {
      "__name": "SESSION_LENGTH",
      "__format": "number"
    },
    "name": "SESSION_NAME",
    "secret": "SESSION_SECRET",
    "redis": {
      "host": "SESSION_REDIS_HOST",
      "port": "SESSION_REDIS_PORT",
      "password": "SESSION_REDIS_PASSWORD",
      "database": "SESSION_REDIS_DB",
      "cluster": "SESSION_REDIS_CLUSTER",
      "useEncryption": {
        "__name": "SESSION_REDIS_USE_ENCRYPTION",
        "__format": "boolean"
      },
      "kmsEndpoint": "SESSION_KMS_ENDPOINT",
      "kmsId": "SESSION_REDIS_KMS_ID",
      "awsRegion": "SESSION_REDIS_AWS_REGION",
      "prefix": "SESSION_REDIS_PREFIX",
      "useTLS": {
        "__name": "SESSION_REDIS_USE_TLS",
        "__format": "boolean"
      }
    }
  }
}
