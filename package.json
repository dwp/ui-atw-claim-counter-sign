    "connect-redis": "8.0.1",
    "cookie-parser": "1.4.7",
    "debug": "4.4.0",
    "express": "4.21.2",
    "express-session": "1.18.1",
    "graphlib": "2.1.8",
    "graphlib-dot": "0.6.4",
    "hmrc-frontend": "5.66.0",
    "ioredis": "5.4.2",
    "luxon": "3.5.0",
    "minimatch": "10.0.1",
    "minimist": "1.2.8",
    "moment": "2.30.1",
    "nhsuk-frontend": "8.3.0",
    "pino": "9.6.0",
    "session-file-store": "1.5.0",
